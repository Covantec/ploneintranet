<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="ploneintranet">

  <body class="view-secure">
    <metal:content fill-slot="content">


      <div tal:condition="here/@@is_fallback_theme"
           id="viewlet-above-content">
        <h1 i18n:translate="not_supported_title">Not supported</h1>
        <p i18n:translate="not_supported_msg">
          This page is not supported in unstyled default Plone.
        </p>
    </div>

    </metal:content>
  </body>
</html>
