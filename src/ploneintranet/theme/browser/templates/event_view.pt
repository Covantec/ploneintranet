                            <input type="time" size="10" class="date" name="start-time" placeholder="Time" value="${context/start}" />
                        </fieldset>
                        <fieldset class="group six columns">
                            <legend>To</legend>
                            <input type="date" size="10" class="date" name="end_date-day" placeholder="Date" value="${context/end}"/>
                            <input type="time" size="10" class="date" name="end_date-time" placeholder="Time" value="${context/end}"/>
                        </fieldset>
                    </fieldset>
